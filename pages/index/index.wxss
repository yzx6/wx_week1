/**index.wxss**/
.container{
  width: 100%;
  background: #F2F2F2;
}
.banner{
  width: 100%;
  height: 300rpx;
  text-align: center;
  background: #5E3862;
  margin-bottom: 140rpx;
}
.banner image{
  width: 90%;
}
.main{
  margin: 0 auto;
  width: 90%;
  height: 293rpx;
  background: #fff;
  display: flex;
  flex-direction: column;
  margin-bottom: 30rpx;
}
.main view{
  flex: 1;
  width: 100%;
  line-height: 100rpx;
  border-bottom: 1rpx solid #ccc;
  padding-left: 20rpx;
  box-sizing: border-box;
}