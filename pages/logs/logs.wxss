.log-list {
  display: flex;
  flex-direction: column;
  background: #f2f2f2;
}
.head{
  display: flex;
  width: 90%;
  background: #fff;
  padding: 40rpx 20rpx;
  margin: 30rpx auto;
}
.date{
 width: 90%;
  background: #fff;
  margin: 0 auto;
  padding: 40rpx 20rpx;
}

 .dates{
  display: inline-block;
  border: 1rpx solid #000;
  margin: 20rpx 20rpx;
  padding: 10rpx;
  border-radius: 10rpx;
}
.bg{
  background: #f00;
  display: inline-block;
  border: 1rpx solid #000;
  margin: 20rpx 20rpx;
  padding: 10rpx;
  border-radius: 10rpx;
}
